# shell=bash
# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Rate Limiting Configuration
VITE_RATE_LIMIT_MAX_REQUESTS=10
VITE_RATE_LIMIT_BLOCK_DURATION=300000

# Cache Configuration
VITE_CACHE_MAX_AGE=30000
VITE_CACHE_MAX_SIZE=1000

# Security Configuration
VITE_MAX_CONTENT_LENGTH=50000
VITE_ALLOWED_DOMAINS=example.com,test.com
VITE_ENCRYPTION_KEY=your_32_character_encryption_key

# Monitoring Configuration
VITE_MONITORING_ENABLED=true
VITE_MONITORING_FLUSH_INTERVAL=60000
VITE_ERROR_SAMPLING_RATE=1.0 